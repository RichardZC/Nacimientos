

@model List<BE.matrimonio_anexo>

@foreach (var item in Model)
{
    <div class="row">
        @if (@ViewBag.editar)
        {
            <div class="col s11 m11">
                <iframe src="~/Actas/Matrimonio/@item.url" style="width:100%;height:400px;" frameborder="0"></iframe>
            </div>
            <div class="col s1 m1 center">
                <a onclick="EliminarAnexo(@item.Matrimonio_AnexoId)" class="btn-floating waves-effect waves-light "><i class="mdi-content-clear"></i></a>
                <p>Eliminar</p>
            </div>
        }
        else
        {
            <iframe src="~/Actas/Matrimonio/@item.url" style="width:100%;height:400px;" frameborder="0"></iframe>
        }
    </div>
}
